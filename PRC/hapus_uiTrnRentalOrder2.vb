        Me.DgvTrnOrderdetil.EndEdit()
        Me.BindingContext(Me.tbl_TrnOrderdetil).EndCurrentEdit()
        tbl_TrnOrderdetil_Changes = Me.tbl_TrnOrderdetil.GetChanges()

